page {
    background-color: #F4F4F4;
}

.goodsori {
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
    height: 288rpx;
    background-color: #ffffff;
}

.goodsori>view:nth-child(1) {
    display: flex;
    justify-content: space-between;
    height: 65rpx;
    align-items: center;
    border-bottom: 1rpx solid #efefef;
}

.goodsori>view:nth-child(1) view:nth-child(1) {
    color: #999999;
    font-size: 28rpx;
}

.goodsori>view:nth-child(1) view:nth-child(2) {
    color: #F15A24;
    font-size: 32rpx;
}

.goodsori>view:nth-child(2) {
    display: flex;
    height: 222rpx;
    align-items: center;
    box-sizing: border-box;
    padding-top: 29rpx;
    padding-bottom: 29rpx;
}

.goodsori>view:nth-child(2)>view {
    height: 100%;
}

.goodsori>view:nth-child(2) image {
    width: 166rpx;
    height: 100%;
    border-radius: 24rpx;
}

.goodsori>view:nth-child(2)>view:nth-child(1) {
    display: flex;
    align-items: center;
}

.goodsori>view:nth-child(2)>view:nth-child(2) {
    padding-left: 20rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
}

.goodsori>view:nth-child(2)>view:nth-child(2)>view:nth-child(1) {
    font-size: 32rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 6;
}

.goodsori>view:nth-child(2)>view:nth-child(2)>view:nth-child(2) {
    font-size: 48rpx;
    color: #F15A24;
    font-family: MoveClubFont;
}

.goodsoriauction {
    height: 376rpx;
}

.goodsoriauction>view:nth-child(3) {
    border-top: 1rpx solid #efefef;
    height: 87rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.goodsoriauction>view:nth-child(3) view {
    color: #999999;
    font-size: 28rpx;
    font-family: MoveClubFont;
}

.goodsoriauction>view:nth-child(3) view text {
    font-size: 36rpx;
    color: #F15A24;
}

.disabled {
    color: #999999!important;
}

.goodsori>view:nth-child(2)>view:nth-child(1) {
    flex: 1;
}

.goodsori>view:nth-child(2)>view:nth-child(2) {
    flex: 3;
}

.pointamount {
    width: 100%;
    display: inline-flex;
    justify-content: space-between;
    align-items: center;
}

.pointamount>view:nth-child(1) {
    display: flex;
    align-items: center;
}

.pointamount>view:nth-child(1) image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 5rpx;
}

.pointamount>view:nth-child(2) {
    font-size: 32rpx;
    color: #393940;
}

.property {
    font-size: 28rpx;
    color: #AAAAAA;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 500rpx;
    display: block;
}